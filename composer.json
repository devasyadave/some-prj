{
    "name": "test/plugin",
    "description": "Provides functionality to plugin",
    "keywords": ["environment","hhvm","xdebug"],
    "homepage": "https://www.google.com",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "devasyadave",
            "email": "devasyadave@gmail.com"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true,
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "suggest": {
        "ext-posix": "*"
    },
    "autoload": {
        "psr-4": {
            "miniorange\\sso\\": "src/"
        }
        
    },
    "extra": {
        "laravel":{
        	"providers": [
        		"miniorange\\sso\\ssoServiceProvider"]
        	}
        },
    "minimum-stability": "dev"
}

